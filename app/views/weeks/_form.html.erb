<%= simple_form_for(@week) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :week_start %>
    <%= f.input :week_end %>
    <%= f.input :season %>
    <%= f.association :year %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
